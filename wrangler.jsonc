{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cloudflare-voicebots",
  "main": "src/server.ts",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "Chat"
      },
      {
        "name": "voicechat",
        "class_name": "VoiceAgent"
      },
      {
        "name": "CONVERSATION", 
        "class_name": "ConversationDO"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "VoiceAgent",
        "ConversationDO"
      ]
    },
    {
      "tag": "v2",
      "new_sqlite_classes": [
        "Chat"
      ]
    }
  ],
  "observability": {
    "enabled": true
  }
}